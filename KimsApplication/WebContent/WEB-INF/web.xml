<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>register</servlet-name>
    <servlet-class>com.kims.DoctorRegister</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/example</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.kims.DoctorLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/doctor_login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>patientregister</servlet-name>
    <servlet-class>com.kims.PatientRegistration</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>patientregister</servlet-name>
    <url-pattern>/patient</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>patientlogin</servlet-name>
    <servlet-class>com.kims.PatientLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>patientlogin</servlet-name>
    <url-pattern>/patient_login</url-pattern>
  </servlet-mapping>
  
</web-app>